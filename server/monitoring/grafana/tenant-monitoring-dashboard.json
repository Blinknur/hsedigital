{
  "dashboard": {
    "title": "HSE Digital - Tenant Monitoring",
    "uid": "hse-tenant-monitoring",
    "version": 1,
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "tags": ["tenant", "multi-tenant", "organization"],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "label": "Tenant/Organization",
          "query": "label_values(hse_digital_tenant_requests_total, tenant_id)",
          "refresh": 1,
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "interval",
          "type": "interval",
          "label": "Time Interval",
          "auto": true,
          "auto_count": 30,
          "auto_min": "10s",
          "options": ["1m", "5m", "10m", "30m", "1h", "6h", "12h", "1d"]
        }
      ]
    },
    "panels": [
      {
        "title": "Tenant Overview",
        "type": "row",
        "id": 100,
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "API Requests by Tenant",
        "type": "timeseries",
        "id": 1,
        "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_api_calls_total{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "API Requests by Method",
        "type": "timeseries",
        "id": 2,
        "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_api_calls_total{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id, method)",
            "legendFormat": "{{tenant_id}} - {{method}}"
          }
        ]
      },
      {
        "title": "Request Latency Percentiles",
        "type": "timeseries",
        "id": 3,
        "gridPos": { "x": 0, "y": 9, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id, le))",
            "legendFormat": "{{tenant_id}} - p50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id, le))",
            "legendFormat": "{{tenant_id}} - p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id, le))",
            "legendFormat": "{{tenant_id}} - p99"
          }
        ]
      },
      {
        "title": "Error Rate by Tenant",
        "type": "timeseries",
        "id": 4,
        "gridPos": { "x": 12, "y": 9, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_errors_total{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Feature Usage by Tenant",
        "type": "timeseries",
        "id": 5,
        "gridPos": { "x": 0, "y": 17, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\"}[$interval])) by (tenant_id, feature)",
            "legendFormat": "{{tenant_id}} - {{feature}}"
          }
        ]
      },
      {
        "title": "Data Storage by Tenant",
        "type": "timeseries",
        "id": 6,
        "gridPos": { "x": 12, "y": 17, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(hse_digital_tenant_data_storage_bytes{tenant_id=~\"$tenant_id\"}) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Concurrent Users",
        "type": "timeseries",
        "id": 7,
        "gridPos": { "x": 0, "y": 25, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "hse_digital_tenant_concurrent_users{tenant_id=~\"$tenant_id\"}",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Resource Cost Estimate",
        "type": "timeseries",
        "id": 8,
        "gridPos": { "x": 12, "y": 25, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(hse_digital_tenant_resource_cost_estimate{tenant_id=~\"$tenant_id\"}) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Performance Score",
        "type": "gauge",
        "id": 9,
        "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "hse_digital_tenant_performance_score{tenant_id=~\"$tenant_id\"}",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Quota Usage",
        "type": "gauge",
        "id": 10,
        "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "hse_digital_tenant_quota_usage_percent{tenant_id=~\"$tenant_id\"}",
            "legendFormat": "{{tenant_id}} - {{quota_type}}"
          }
        ]
      }
    ]
  }
}
