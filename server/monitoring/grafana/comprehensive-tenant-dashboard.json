{
  "dashboard": {
    "title": "HSE Digital - Comprehensive Tenant Analytics",
    "uid": "hse-comprehensive-tenant",
    "version": 1,
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "tags": ["tenant", "comprehensive", "analytics", "multi-tenant"],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "1m",
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "label": "Tenant/Organization",
          "query": "label_values(hse_digital_tenant_api_calls_total, tenant_id)",
          "refresh": 1,
          "multi": false,
          "includeAll": false
        },
        {
          "name": "interval",
          "type": "interval",
          "label": "Time Interval",
          "auto": true,
          "auto_count": 30,
          "auto_min": "10s",
          "options": ["1m", "5m", "10m", "30m", "1h", "6h"]
        }
      ]
    },
    "panels": [
      {
        "title": "Tenant Health Summary",
        "type": "row",
        "id": 100,
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Performance Score",
        "type": "stat",
        "id": 1,
        "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "hse_digital_tenant_performance_score{tenant_id=\"$tenant_id\"}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 50, "color": "yellow" },
                { "value": 70, "color": "green" }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "title": "Requests (24h)",
        "type": "stat",
        "id": 2,
        "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(increase(hse_digital_tenant_api_calls_total{tenant_id=\"$tenant_id\"}[24h]))"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "title": "Error Rate",
        "type": "stat",
        "id": 3,
        "gridPos": { "x": 12, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "(sum(rate(hse_digital_tenant_errors_total{tenant_id=\"$tenant_id\"}[5m])) / sum(rate(hse_digital_tenant_api_calls_total{tenant_id=\"$tenant_id\"}[5m]))) * 100"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "title": "Avg Response Time",
        "type": "stat",
        "id": 4,
        "gridPos": { "x": 18, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_latency_seconds_sum{tenant_id=\"$tenant_id\"}[5m])) / sum(rate(hse_digital_tenant_latency_seconds_count{tenant_id=\"$tenant_id\"}[5m]))"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "title": "API Usage Patterns",
        "type": "row",
        "id": 101,
        "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "API Request Rate",
        "type": "timeseries",
        "id": 5,
        "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_api_calls_total{tenant_id=\"$tenant_id\"}[$interval])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ]
      },
      {
        "title": "API Methods Distribution",
        "type": "piechart",
        "id": 6,
        "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(increase(hse_digital_tenant_api_calls_total{tenant_id=\"$tenant_id\"}[1h])) by (method)",
            "legendFormat": "{{method}}"
          }
        ]
      },
      {
        "title": "Feature Adoption",
        "type": "row",
        "id": 102,
        "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Feature Usage",
        "type": "timeseries",
        "id": 7,
        "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=\"$tenant_id\"}[$interval])) by (feature)",
            "legendFormat": "{{feature}}"
          }
        ]
      },
      {
        "title": "Top Features (24h)",
        "type": "bargauge",
        "id": 8,
        "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "topk(10, sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=\"$tenant_id\"}[24h])) by (feature))",
            "legendFormat": "{{feature}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "title": "Performance Metrics",
        "type": "row",
        "id": 103,
        "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Response Time Percentiles",
        "type": "timeseries",
        "id": 9,
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=\"$tenant_id\"}[$interval])) by (le))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=\"$tenant_id\"}[$interval])) by (le))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(hse_digital_tenant_latency_seconds_bucket{tenant_id=\"$tenant_id\"}[$interval])) by (le))",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "Error Types",
        "type": "timeseries",
        "id": 10,
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_errors_total{tenant_id=\"$tenant_id\"}[$interval])) by (error_type)",
            "legendFormat": "{{error_type}}"
          }
        ]
      },
      {
        "title": "Resource Usage & Cost",
        "type": "row",
        "id": 104,
        "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Data Storage",
        "type": "timeseries",
        "id": 11,
        "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(hse_digital_tenant_data_storage_bytes{tenant_id=\"$tenant_id\"}) by (data_type) / 1024 / 1024",
            "legendFormat": "{{data_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes"
          }
        }
      },
      {
        "title": "Estimated Costs",
        "type": "timeseries",
        "id": 12,
        "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(hse_digital_tenant_resource_cost_estimate{tenant_id=\"$tenant_id\"}) by (resource_type)",
            "legendFormat": "{{resource_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        }
      },
      {
        "title": "Quota Usage",
        "type": "gauge",
        "id": 13,
        "gridPos": { "x": 0, "y": 41, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "hse_digital_tenant_quota_usage_percent{tenant_id=\"$tenant_id\"}",
            "legendFormat": "{{quota_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 70, "color": "yellow" },
                { "value": 90, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "title": "Concurrent Users",
        "type": "timeseries",
        "id": 14,
        "gridPos": { "x": 12, "y": 41, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "hse_digital_tenant_concurrent_users{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Active Users"
          }
        ]
      }
    ]
  }
}
