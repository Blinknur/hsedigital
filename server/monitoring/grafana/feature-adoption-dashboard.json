{
  "dashboard": {
    "title": "HSE Digital - Feature Adoption",
    "uid": "hse-feature-adoption",
    "version": 1,
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "tags": ["features", "adoption", "usage"],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "query",
          "label": "Tenant/Organization",
          "query": "label_values(hse_digital_tenant_feature_usage_total, tenant_id)",
          "refresh": 1,
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "feature",
          "type": "query",
          "label": "Feature",
          "query": "label_values(hse_digital_tenant_feature_usage_total, feature)",
          "refresh": 1,
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "title": "Feature Usage Overview",
        "type": "row",
        "id": 100,
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Feature Usage by Tenant",
        "type": "timeseries",
        "id": 1,
        "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[5m])) by (tenant_id, feature)",
            "legendFormat": "{{tenant_id}} - {{feature}}"
          }
        ]
      },
      {
        "title": "Most Popular Features",
        "type": "bargauge",
        "id": 2,
        "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "topk(10, sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[24h])) by (feature))",
            "legendFormat": "{{feature}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "title": "Feature Adoption Rate",
        "type": "stat",
        "id": 3,
        "gridPos": { "x": 0, "y": 9, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "count(count(hse_digital_tenant_feature_usage_total{feature=~\"$feature\"}) by (tenant_id))",
            "legendFormat": "Tenants Using Features"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        }
      },
      {
        "title": "Total Feature Usage Count",
        "type": "stat",
        "id": 4,
        "gridPos": { "x": 8, "y": 9, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[30d]))",
            "legendFormat": "Total Usage"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "title": "Active Features per Tenant",
        "type": "stat",
        "id": 5,
        "gridPos": { "x": 16, "y": 9, "w": 8, "h": 6 },
        "targets": [
          {
            "expr": "count(count(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\"}) by (tenant_id, feature)) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background"
        }
      },
      {
        "title": "Feature Usage Distribution",
        "type": "piechart",
        "id": 6,
        "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[7d])) by (feature)",
            "legendFormat": "{{feature}}"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" },
          "pieType": "pie"
        }
      },
      {
        "title": "Feature Usage Heatmap",
        "type": "heatmap",
        "id": 7,
        "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[1h])) by (feature)",
            "format": "heatmap"
          }
        ],
        "options": {
          "calculate": true
        }
      },
      {
        "title": "Audit Features",
        "type": "row",
        "id": 101,
        "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Audit Creation Rate",
        "type": "timeseries",
        "id": 8,
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=\"audit_create\"}[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Audit Completion Rate",
        "type": "timeseries",
        "id": 9,
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=\"audit_complete\"}[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Incident Management Features",
        "type": "row",
        "id": 102,
        "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Incident Reporting Rate",
        "type": "timeseries",
        "id": 10,
        "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=\"incident_create\"}[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Incident Resolution Rate",
        "type": "timeseries",
        "id": 11,
        "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=\"incident_resolve\"}[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}"
          }
        ]
      },
      {
        "title": "Station & Contractor Features",
        "type": "row",
        "id": 103,
        "gridPos": { "x": 0, "y": 41, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "Station Management Usage",
        "type": "timeseries",
        "id": 12,
        "gridPos": { "x": 0, "y": 42, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"station_.*\"}[5m])) by (tenant_id, feature)",
            "legendFormat": "{{tenant_id}} - {{feature}}"
          }
        ]
      },
      {
        "title": "Contractor Management Usage",
        "type": "timeseries",
        "id": 13,
        "gridPos": { "x": 12, "y": 42, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"contractor_.*\"}[5m])) by (tenant_id, feature)",
            "legendFormat": "{{tenant_id}} - {{feature}}"
          }
        ]
      },
      {
        "title": "Feature Adoption Trends",
        "type": "row",
        "id": 104,
        "gridPos": { "x": 0, "y": 50, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "title": "New Feature Adoption",
        "type": "table",
        "id": 14,
        "gridPos": { "x": 0, "y": 51, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "topk(10, sum(increase(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\"}[7d])) by (feature))",
            "format": "table",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Value",
              "desc": true
            }
          ]
        }
      },
      {
        "title": "Feature Growth Rate",
        "type": "timeseries",
        "id": 15,
        "gridPos": { "x": 12, "y": 51, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(hse_digital_tenant_feature_usage_total{tenant_id=~\"$tenant_id\", feature=~\"$feature\"}[1h])) by (feature)",
            "legendFormat": "{{feature}}"
          }
        ]
      }
    ]
  }
}
